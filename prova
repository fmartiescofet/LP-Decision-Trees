p,x,n,k\ne,x,y,k\ne,b,w,n\np,x,w,n\ne,x,y,n\ne,b,w,n\np,x,w,p

contents = "p,x,n,k\ne,x,y,k\ne,b,w,n\np,x,w,n\ne,x,y,n\ne,b,w,n\np,x,w,p"
let d = transpose $ lines $ filter (/=',') contents
attributes = ["cap-shape","cap-color","gill-color"]

let classification = head d
let dat = tail d
d = dat
arbre = buildDTree attributes classification dat
print arbre
map (computeAccuracy classification) d


let classification (Leaf s) = do
        putStrLn s
    let classification (Node attr l) = do
        putStrLn $ "Which " ++ attr ++ "?"
        char <- getChar
        let tree2 = lookup char l
        case tree2 of
          Just a -> classification a
          Nothing -> classification (Node attr l)



contents <- readFile "short.data"
    let attributes = ["cap-shape","cap-color","gill-color"]